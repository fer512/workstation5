<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity OrderQueue.
    -->
    <changeSet id="20230620195911-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="queue_id"
                                 baseTableName="order_queue"
                                 constraintName="fk_order_queue__queue_id"
                                 referencedColumnNames="id"
                                 referencedTableName="queue"
                                 />

        <addForeignKeyConstraint baseColumnNames="worker_profile_id"
                                 baseTableName="order_queue"
                                 constraintName="fk_order_queue__worker_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="worker_profile"
                                 />
    </changeSet>
</databaseChangeLog>
